<h1>Commissioner Access</h1>

<% if current_user %>
   <h3><%= "You are currently logged in with commissioner access" %></h3>
<% else %>
<h3>Log In</h3>
  <%= form_tag("/login", method: "post") do %>
    <%= email_field_tag :email, nil, placeholder: "Email" %>
    <br>
    <%= password_field_tag :password, nil, placeholder: "Password" %>
    <br><br>
    <%= submit_tag "Log In" %><br><br>
  <% end %>
<% end %>